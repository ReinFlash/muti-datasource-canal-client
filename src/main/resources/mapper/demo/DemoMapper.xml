<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xyt.canalclient.consumer.demo.mapper.DemoMapper">

    <select id="selectMallDeposit" resultType="java.util.Map">
        select * from mall_deposit where id = #{id}
    </select>

    <select id="selectBook" resultType="java.util.Map">
        select * from book where id = #{id}
    </select>
</mapper>